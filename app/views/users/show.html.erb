<div class="container">
  <h1>Profil de <%= @user.first_name%> <%= @user.last_name %></h1>
  <ul>
    <li>
      <%= @user.gender  %>
    </li>
    <li>
      <%= @user.city  %>
    </li>
    <li>
      <%= @user.age %> ans
    </li>
    <li>
      <%= @user.email  %>
    </li>
    <li>
      <%= @user.phone_number  %>
    </li>
  </ul>



  <%= link_to "Modifier profil", edit_user_registration_path(@user) %>
  <br>
  <%= link_to "Retour", :back %>

</div>
